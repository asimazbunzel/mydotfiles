#!/bin/sh

compasinit () {

   case $1 in 
      
      default)
         export COMPAS_ROOT_DIR=~/Developments/COMPAS
         ;;

      *)
         echo "unknown option: $1. only avaible is 'default'"
         ;;
   esac
}

_compasinit() {
   compadd default
}


# if not defined here, compdef will not be found
autoload -Uz compinit
compinit

compdef _compasinit compasinit
